<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="cn.miaole.aircraft_booking_android">

    <uses-permission android:name="android.permission.INTERNET" />
    <!-- 定位相关权限 -->
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@drawable/icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar"
        tools:ignore="UnusedAttribute">
        <activity
            android:name=".activitys.main.MainActivity"
            android:launchMode="singleTask" />
        <activity android:name=".activitys.welcome.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activitys.login.LoginActivity" />
        <activity android:name=".activitys.register.RegisterActivity" />
        <activity android:name=".activitys.forget_pwd.ForgetPwdActivity" />
        <activity android:name=".activitys.my_order.MyOrderActivity" />
        <activity android:name=".activitys.my_trip.MyTripActivity" />
        <activity android:name=".activitys.about_us.AboutUsActivity" />
        <activity android:name=".activitys.passengers.PassengersActivity" />
        <activity android:name=".activitys.add_passenger.AddPassengerActivity" />
        <activity android:name=".activitys.place_select.PlaceSelectActivity" />
        <activity android:name=".activitys.ticket_search_result.TicketSearchResultActivity"></activity>
    </application>

</manifest>